{
    "security.allowedUNCHosts": [
        "wsl.localhost"
    ],
    "editor.minimap.enabled": false,
    "editor.fontFamily": "'Fira Code Light_', 'JetBrains Mono_', 'Cascadia Code', Consolas, 'Courier New', monospace",
    "editor.fontLigatures": false,
    "editor.cursorBlinking": "phase",
    "editor.cursorStyle": "underline",
    "editor.scrollbar.vertical": "hidden",
    "editor.scrollbar.horizontal": "hidden",
    "editor.guides.indentation": false,
    "editor.hideCursorInOverviewRuler": true,
    "editor.tabSize": 4,
    "editor.insertSpaces": true,
    "editor.detectIndentation": false,
    "editor.autoIndent": "full",
    "editor.wordWrap": "off",
    "editor.formatOnSave": true,
    "editor.matchBrackets": "never",
    "editor.overviewRulerBorder": false,
    "editor.renderLineHighlight": "none",
    "editor.lightbulb.enabled": "off",
    "editor.renderWhitespace": "none",
    "editor.occurrencesHighlight": "off",
    "editor.unicodeHighlight.nonBasicASCII": false,
    "editor.stickyScroll.enabled": false,
    "editor.bracketPairColorization.enabled": false,
    "editor.guides.highlightActiveBracketPair": false,
    "editor.guides.bracketPairsHorizontal": false,
    "editor.glyphMargin": false,
    "editor.folding": false,
    "editor.lineNumbers": "on",
    "editor.lineDecorationsWidth": 10,
    "editor.showFoldingControls": "never",
    "editor.cursorSmoothCaretAnimation": "on",
    "editor.rulers": [
        {
            "column": 80,
            "color": "#85a0db18",
        }
    ],
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "window.commandCenter": false,
    "window.title": "No Code No Life${separator}${profileName}${separator}${dirty}${activeEditorShort}${separator}${rootName}${separator}${separator}",
    "explorer.decorations.colors": false,
    "explorer.autoReveal": false,
    "explorer.autoOpenDroppedFile": false,
    "explorer.compactFolders": true,
    "zenMode.showTabs": "none",
    "zenMode.centerLayout": false,
    "terminal.integrated.cursorStyle": "underline",
    "terminal.integrated.cursorBlinking": true,
    "breadcrumbs.filePath": "off",
    "breadcrumbs.enabled": false,
    "workbench.layoutControl.enabled": false,
    "workbench.editor.labelFormat": "short",
    "workbench.editor.showTabs": "none",
    "workbench.editor.tabActionCloseVisibility": false,
    "workbench.tree.renderIndentGuides": "none",
    "workbench.editor.enablePreview": false,
    "workbench.editor.editorActionsLocation": "hidden",
    "workbench.layoutControl.type": "menu",
    "workbench.sideBar.location": "right",
    "workbench.startupEditor": "none",
    "workbench.tips.enabled": false,
    "workbench.editorAssociations": {
        "*.copilotmd": "vscode.markdown.preview.editor",
        "*.svg": "default"
    },
    "workbench.colorCustomizations": {
        "editorOverviewRuler.errorForeground": "#00000000",
        "editorOverviewRuler.background": "#00000000",
        "editorOverviewRuler.warningForeground": "#00000000",
        "editorOverviewRuler.infoForeground": "#00000000",
        "editorOverviewRuler.bracketMatchForeground": "#00000000",
        "editorOverviewRuler.findMatchForeground": "#00000000",
        "editorOverviewRuler.rangeHighlightForeground": "#00000000",
        "editorOverviewRuler.selectionHighlightForeground": "#00000000",
        "editorOverviewRuler.wordHighlightForeground": "#00000000",
        "editorOverviewRuler.wordHighlightStrongForeground": "#00000000",
        "editorOverviewRuler.modifiedForeground": "#00000000",
        "editorOverviewRuler.addedForeground": "#00000000",
        "editorOverviewRuler.deletedForeground": "#00000000",
        "editorGutter.background": "#00000000",
        // "editor.wordHighlightStrongBackground": "#C63D2F",
        // "editor.wordHighlightStrongBorder": "#C63D2F",
        // "editor.wordHighlightBackground": "#352F44",
        // "editor.wordHighlightBorder": "#352F44",
        // "editor.selectionBackground": "#42293f",
        // "editorCursor.foreground": "#ffffff"
    },
    "git.enabled": false,
    "git.decorations.enabled": false,
    "typescript.updateImportsOnFileMove.enabled": "always",
    "typescript.validate.enable": false,
    "javascript.updateImportsOnFileMove.enabled": "always",
    "css.format.spaceAroundSelectorSeparator": true,
    "powermode.enabled": true,
    "powermode.combo.location": "off",
    "powermode.combo.counterEnabled": "hide",
    "powermode.shake.enabled": false,
    "background.windowBackgrounds": [
        "c:/Users/histo/ScandalCode/Wallpapers/424.png"
    ],
    "background.backgroundOpacity": [
        0.1,
        0.1,
        0.1,
        0.1
    ],
    "cSpell.language": "en,es",
    "cSpell.dictionaries": [
        "english",
        "spanish"
    ],
    "shellformat.path": "C:\\bin\\shfmt.exe",
    "shellformat.flag": "-i 2 -ci -sr -bn",
    "python.formatting.provider": "black",
    "python.formatting.blackArgs": [
        "--line-length=88"
    ],
    "python.linting.enabled": true,
    "python.linting.pylintEnabled": true,
    "python.linting.pylintArgs": [
        "--rcfile=${workspaceFolder}/.pylintrc"
    ],
    "python.languageServer": "Pylance",
    "python.analysis.typeCheckingMode": "basic",
    "python.analysis.autoImportCompletions": true,
    "python.analysis.diagnosticMode": "workspace",
    "gruvboxConcoctis.useItalicFont": false,
    "catppuccin.italicComments": false,
    "catppuccin.italicKeywords": false,
    "window.menuBarVisibility": "compact",
    "redhat.telemetry.enabled": true,
    "debug.breakpointsView.presentation": "tree",
    "workbench.colorTheme": "Catppuccin Mocha",
    "workbench.productIconTheme": "el-vsc-v1-icons",
    "workbench.iconTheme": "catppuccin-mocha",
    "workbench.statusBar.visible": false,
    "workbench.activityBar.location": "hidden",
    "json.schemaDownload.trustedDomains": {
        "https://schemastore.azurewebsites.net/": true,
        "https://raw.githubusercontent.com/": true,
        "https://www.schemastore.org/": true,
        "https://json.schemastore.org/": true,
        "https://json-schema.org/": true,
        "https://esm.sh/": true
    }
}
